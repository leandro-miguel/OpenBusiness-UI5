<!--LP-->
<mvc:View
	controllerName="requisitions.requisitions.controller.order.Create"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:m="sap.m"
	xmlns:core="sap.ui.core"
	xmlns="sap.uxap"
	height="100%"
	xmlns:layout="sap.ui.layout"
	xmlns:ui="sap.ui.table"
	xmlns:html="http://www.w3.org/1999/xhtml"
>
	<ObjectPageLayout
		id="ObjectPageLayout"
		enableLazyLoading="true"
		upperCaseAnchorBar="false"
		showFooter="true"
	>
		<headerTitle>
			<ObjectPageDynamicHeaderTitle>
				<expandedHeading>
					<m:Title
						text="{oModelCreateOrder>/ViewTitle}"
						wrapping="true"
					/>
				</expandedHeading>
				<snappedHeading>
					<m:Title
						text="{oModelCreateOrder>/ViewTitle}"
						wrapping="true"
					/>
				</snappedHeading>
				<expandedContent>
					<m:VBox>
						<m:Text
							text="{ path: 'oModel>/currentDate', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd/MM/yyyy - HH:mm:ss' } }"
							visible="{oModelCreateOrder>/CreateVisible}"
						/>
						<m:Text
							text="{i18n>OrderNum}: {oModelCreateOrder>/DocEntryDocumento} {oModelCreateOrder>/numSAP} "
							visible="{oModelCreateOrder>/EditVisible}"
						/>
					</m:VBox>
				</expandedContent>
				<actions>
					<m:Button
						icon="sap-icon://pdf-attachment"
						type="Emphasized"
						press=".downloadPDF"
						visible="{oModelCreateOrder>/EditVisible}"
					/>
					<m:Button
						text="{i18n>update}"
						type="Emphasized"
						press=".updateUserData"
						visible="{oModelCreateOrder>/updateUserDataVisible}"
					/>
					<m:Button
						text="{i18n>saveOrder}"
						type="Emphasized"
						press=".onCreateOrder"
						visible="{oModelCreateOrder>/CreateVisible}"
					/>
					<m:Button
						text="{i18n>edit}"
						type="Emphasized"
						press=".editOrder"
						enabled="{oModelCreateOrder>/ApprovedEnabled}"
						visible="{oModelCreateOrder>/EditVisible}"
					/>
					<m:Button
						text="{i18n>cancel}"
						type="Default"
						press=".cancelOrder"
						visible="{oModelCreateOrder>/CreateVisible}"
					/>
					<m:Button text="{i18n>return}" type="Default" press=".navBack" visible="{oModelCreateOrder>/EditVisible}"/>
				</actions>
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>
		<sections>
			<ObjectPageSection
				titleUppercase="false"
				title="{i18n>generalInformation}"
			>
				<subSections>
					<ObjectPageSubSection
						titleUppercase="false"
						title="{i18n>generalInformation}"
						mode="Expanded"
					>
						<blocks>
							<layout:HorizontalLayout allowWrapping="true">
								<layout:VerticalLayout class="sapUiMediumMarginEnd">
									<m:Label
										text="{i18n>company}"
										labelFor="selectEmpresa"
										required="true"
									/>
									<m:Select
										id="selectEmpresa"
										width="20rem"
										selectedKey="{oModelCreateOrder>/companySelectedKey}"
										forceSelection="false"
										valueState="{oModelCreateOrder>/companySelectedKeyState}"
										change="onCompanySelect"
										items="{ path: 'oModel>/Companies', sorter: { path: 'IdEmpresas' } }"
										busy="{oModel>/companySelectBusy}"
										editable="{oModelCreateOrder>/CompanyEditable}"
									>
										<core:Item
											key="{oModel>IdEmpresas}"
											text="{oModel>Empresas}"
										/>
									</m:Select>
									<m:Label
										text="{i18n>branch}"
										labelFor="filial"
										required="true"
									/>
									<m:Select
										id="filial"
										width="20rem"
										forceSelection="false"
										change="onBranchSelect"
										selectedKey="{oModelCreateOrder>/branch}"
										valueState="{oModelCreateOrder>/branchState}"
										enabled="{oModelCreateOrder>/Enabled}"
										editable="{oModelCreateOrder>/Editable}"
										items="{ path: 'oModel>/Branches', sorter: { path: 'idFilial' } }"
									>
										<core:Item
											key="{oModel>idFilial}"
											text="{oModel>nomeFilial}"
										/>
									</m:Select>
									<m:Label
										text="{i18n>provider}"
										labelFor="fornecedor"
										required="true"
									/>
									<m:Input
										id="fornecedor"
										width="20rem"
										type="Text"
										showSuggestion="true"
										showValueHelp="true"
										valueHelpRequest="onLinePartnerHelpRequest"
										change="onLinePartnerValueSetManual"
										selectedKey="{oModelCreateOrder>/lineVendor}"
										valueState="{oModelCreateOrder>/vendorState}"
										enabled="{oModelCreateOrder>/PartnerEnabled}"
										editable="{oModelCreateOrder>/Editable}"
										busy="{oModelCreateOrder>/onLineVendorBusy}"
										value="{oModelCreateOrder>/onLineVendorValue}"
									/>
									<m:Label
										text="{i18n>providerDesc}"
										labelFor="fornecedorNome"
									/>
									<m:Input
											id="fornecedorNome" width="20rem"
											value="{oModelCreateOrder>/onLineVendorValueDesc}"
											editable="false"
										/>	
									<m:Label
										text="{i18n>contactPerson}"
										labelFor="selectPessoa"
									/>
									<m:Input
										id="selectPessoa"
										width="20rem"
										class="sapUiSmallMarginBottom"
										type="Text"
										value="{oModelCreateOrder>/contactPerson}"
										busy="{oModelCreateOrder>/contactPersonBusy}"
										editable="false"
									/>
								</layout:VerticalLayout>
							</layout:HorizontalLayout>
							<layout:HorizontalLayout allowWrapping="true">
								<layout:VerticalLayout class="sapUiMediumMarginEnd">
									<m:Label
										text="{i18n>releaseDate}"
										labelFor="dataLancamento"
									/>
									<m:DatePicker
										id="dataLancamento"
										width="20rem"
										valueFormat="yyyy-MM-dd"
										enabled="{oModelCreateOrder>/Enabled}"
										editable="{oModelCreateOrder>/Editable}"
										value="{oModelCreateOrder>/releaseDate}"
									/>
								</layout:VerticalLayout>
								<layout:VerticalLayout class="sapUiMediumMarginEnd">
									<m:Label
										text="{i18n>deliveryDate}"
										labelFor="dataEntrega"
										required="true"
									/>
									<m:DatePicker
										id="dataEntrega"
										width="20rem"
										valueFormat="yyyy-MM-dd"
										value="{oModelCreateOrder>/deliveryDate}"
										enabled="{oModelCreateOrder>/Enabled}"
										editable="{oModelCreateOrder>/Editable}"
										valueState="{oModelCreateOrder>/deliveryDateState}"
									/>
								</layout:VerticalLayout>
								<layout:VerticalLayout class="sapUiMediumMarginEnd">
									<m:Label
										text="{i18n>documentDate}"
										labelFor="dataDocumento"
									/>
									<m:DatePicker
										id="dataDocumento"
										width="20rem"
										valueFormat="yyyy-MM-dd"
										enabled="{oModelCreateOrder>/Enabled}"
										editable="{oModelCreateOrder>/Editable}"
										value="{oModelCreateOrder>/dataDocument}"
									/>
								</layout:VerticalLayout>
							</layout:HorizontalLayout>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection
				titleUppercase="false"
				title="{i18n>itemTitle}"
			>
				<subSections>
					<ObjectPageSubSection titleUppercase="false">
						<blocks>
							<m:VBox>
								<m:Table
									showNoData="false"
									width="auto"
								>
									<m:headerToolbar>
										<m:OverflowToolbar>
											<m:content>
												<m:Title
													id="title2"
													text="{i18n>tableTitle}"
													class="Title"
												/>
												<m:ToolbarSpacer/>
												<!-- <m:Button
													id="editButton"
													text="{i18n>edit}"
													type="Default"
													press=".onEditItem"
													enabled="{oModelCreateOrder>/Editable}"
												/> -->
												<m:Button
													id="copyButton"
													text="{i18n>copy}"
													type="Default"
													press=".onCopyItem"
													enabled="{oModelCreateOrder>/Editable}"
												/>
												<m:Button
													id="deleteButton"
													text="{i18n>remove}"
													type="Default"
													press=".onRemoveItem"
													enabled="{oModelCreateOrder>/Editable}"
												/>
												<!-- Botao de add -->
												<m:Button
													id="addButton"
													icon="sap-icon://add"
													type="Default"
													press=".onAddItem"
													enabled="{oModelCreateOrder>/Editable}"
												/>
											</m:content>
										</m:OverflowToolbar>
									</m:headerToolbar>
								</m:Table>
								<m:ScrollContainer
									height="100%"
									width="100%"
									vertical="true"
									focusable="true"
								>
									<ui:Table
										id="table"
										width="auto"
										rows="{oModelCreateOrder>/rows}"
										visibleRowCount="{oModelCreateOrder>/rowsCount}"
										busy="{oModelCreateOrder>/busyTable}"
										editable="{oModelCreateOrder>/Editable}"
									>
										<ui:columns>
											<ui:Column>
												<m:Label text="{i18n>itemId}"/>
												<ui:template>
													<m:Label
														text="{oModelCreateOrder>itemId}"
														wrapping="false"
													/>
												</ui:template>
											</ui:Column>
											<ui:Column>
												<m:Label
													text="{i18n>itemCode}"
													required="true"
												/>
												<ui:template>
													<m:Input
														showValueHelp="true"
														type="Text"
														showSuggestion="true"
														editable="{oModelCreateOrder>/Editable}"
														valueHelpRequest="onItemNumberHelpRequest"
														change="onItemNumberValueSetManual"
														value="{oModelCreateOrder>itemNumber}"
														busy="{oModelCreateOrder>itemNumberBusy}"
														valueState="{oModelCreateOrder>itemNumberState}"
													/>
												</ui:template>
											</ui:Column>
											<ui:Column>
												<m:Label text="{i18n>itemDescription}"/>
												<ui:template>
													<m:Input
														type="Text"
														editable="false"
														value="{oModelCreateOrder>itemNumberDesc}"
														busy="{oModelCreateOrder>itemNumberBusy}"
													/>
												</ui:template>
											</ui:Column>
											<ui:Column
											visible="{oModelCreateOrder>/Visible}">
												<m:Label text="{i18n>category}"/>
												<ui:template>
													<m:Input
														showValueHelp="true"
														type="Text"
														showSuggestion="true"
														editable="{oModelCreateOrder>/Editable}"
														valueHelpRequest="onCategoryHelpRequest"
														change="onCategoryValueSetManual"
														value="{oModelCreateOrder>category}"
														busy="{oModelCreateOrder>categoryBusy}"
													/>
												</ui:template>
											</ui:Column>
											<ui:Column>
												<m:Label
													text="{i18n>amount}"
													required="true"
												/>
												<ui:template>
													<m:Input
														liveChange="necessaryAmountChange"
														valueState="{oModelCreateOrder>necessaryAmountState}"
														value="{oModelCreateOrder>necessaryAmount}"
														editable="{oModelCreateOrder>/Editable}"
													/>
												</ui:template>
											</ui:Column>
											<ui:Column>
												<m:Label text="{i18n>priceUnit}"/>
												<ui:template>
													<m:Input
														liveChange="priceInfoChange"
														value="{oModelCreateOrder>priceInfo}"
														editable="{oModelCreateOrder>/Editable}"
													/>
												</ui:template>
											</ui:Column>
											<ui:Column>
												<m:Label text="{i18n>discount}"/>
												<ui:template>
													<m:Input
														liveChange="discountPercent"
														value="{oModelCreateOrder>discount}"
														editable="{oModelCreateOrder>/Editable}"
													/>
												</ui:template>
											</ui:Column>
											<ui:Column>
												<m:Label text="{i18n>totalPrice}"/>
												<ui:template>
													<m:Input
														value="{oModelCreateOrder>totalPrice}"
														editable="false"
													/>
												</ui:template>
											</ui:Column>
											<!-- Dimensions -->
											<ui:Column
												id="dimension1"
												visible="{oModelCreateOrder>/dimension1Visible}"
											>
												<m:Label text="{oModelCreateOrder>/dimension1Label}"/>
												<ui:template>
													<m:Input
														showValueHelp="true"
														type="Text"
														showSuggestion="true"
														editable="{oModelCreateOrder>/Editable}"
														valueHelpRequest="onDimensionHelpRequestOrder($event, 1)"
														change="onDimensionValueSetManual($event, 1)"
														value="{oModelCreateOrder>dimension1}"
														busy="{oModelCreateOrder>dimensionBusy1}"
													/>
												</ui:template>
											</ui:Column>
											<ui:Column
												id="dimension2"
												visible="{oModelCreateOrder>/dimension2Visible}"
											>
												<m:Label text="{oModelCreateOrder>/dimension2Label}"/>
												<ui:template>
													<m:Input
														showValueHelp="true"
														type="Text"
														showSuggestion="true"
														editable="{oModelCreateOrder>/Editable}"
														valueHelpRequest="onDimensionHelpRequestOrder($event, 2)"
														change="onDimensionValueSetManual($event, 2)"
														value="{oModelCreateOrder>dimension2}"
														busy="{oModelCreateOrder>dimensionBusy2}"
													/>
												</ui:template>
											</ui:Column>
											<ui:Column
												id="dimension3"
												visible="{oModelCreateOrder>/dimension3Visible}"
											>
												<m:Label text="{oModelCreateOrder>/dimension3Label}"/>
												<ui:template>
													<m:Input
														showValueHelp="true"
														type="Text"
														showSuggestion="true"
														editable="{oModelCreateOrder>/Editable}"
														valueHelpRequest="onDimensionHelpRequestOrder($event, 3)"
														change="onDimensionValueSetManual($event, 3)"
														value="{oModelCreateOrder>dimension3}"
														busy="{oModelCreateOrder>dimensionBusy3}"
													/>
												</ui:template>
											</ui:Column>
											<ui:Column
												id="dimension4"
												visible="{oModelCreateOrder>/dimension4Visible}"
											>
												<m:Label text="{oModelCreateOrder>/dimension4Label}"/>
												<ui:template>
													<m:Input
														showValueHelp="true"
														type="Text"
														showSuggestion="true"
														editable="{oModelCreateOrder>/Editable}"
														valueHelpRequest="onDimensionHelpRequestOrder($event, 4)"
														change="onDimensionValueSetManual($event, 4)"
														value="{oModelCreateOrder>dimension4}"
														busy="{oModelCreateOrder>dimensionBusy4}"
													/>
												</ui:template>
											</ui:Column>
											<ui:Column
												id="dimension5"
												visible="{oModelCreateOrder>/dimension5Visible}"
											>
												<m:Label text="{oModelCreateOrder>/dimension5Label}"/>
												<ui:template>
													<m:Input
														showValueHelp="true"
														type="Text"
														showSuggestion="true"
														editable="{oModelCreateOrder>/Editable}"
														valueHelpRequest="onDimensionHelpRequestOrder($event, 5)"
														change="onDimensionValueSetManual($event, 5)"
														value="{oModelCreateOrder>dimension5}"
														busy="{oModelCreateOrder>dimensionBusy5}"
													/>
												</ui:template>
											</ui:Column>
											<ui:Column>
												<m:Label text="{i18n>projectCode}"/>
												<ui:template>
													<m:Input
														showValueHelp="true"
														type="Text"
														showSuggestion="true"
														editable="{oModelCreateOrder>/Editable}"
														valueHelpRequest="onProjectHelpRequest"
														change="onProjectValueSetManual"
														value="{oModelCreateOrder>projectCode}"
														busy="{oModelCreateOrder>projectCodeBusy}"
													/>
												</ui:template>
											</ui:Column>
											<ui:Column>
												<m:Label text="{i18n>codeUmbrella}"/>
												<ui:template>
													<m:Input
														showValueHelp="true"
														type="Text"
														showSuggestion="true"
														editable="{oModelCreateOrder>/Editable}"
														valueHelpRequest="onContractHelpRequest"
														change="onContractNumValueSetManual"
														value="{oModelCreateOrder>contractName}"
													/>
												</ui:template>
											</ui:Column>
										</ui:columns>
									</ui:Table>
								</m:ScrollContainer>
								<layout:HorizontalLayout allowWrapping="true">
									<layout:VerticalLayout class="sapUiMediumMarginEnd">
										<m:FlexBox
											direction="Column"
											alignItems="Start"
										>
											<m:Label text="{i18n>totalprice}"/>
											<m:Label
												design="Bold"
												text="{oModelCreateOrder>/totalprice}"
												class="sapUiSmallMarginBottom"
											/>
											<m:Label
												text="{i18n>holder}"
												labelFor="titular"
											/>
											<m:Input
												id="titular"
												width="20rem"
												type="Text"
												showSuggestion="true"
												showValueHelp="true"
												valueHelpRequest="onEmployeeHelpRequest('titular')"
												change="onTitularValueSetManual"
												value="{oModelCreateOrder>/documentsOwnerName}"
												busy="{oModelCreateOrder>/documentsOwnerNameBusy}"
												enabled="{oModelCreateOrder>/Enabled}"
												editable="{oModelCreateOrder>/Editable}"
											/>
											<m:Label
												text="{i18n>comments}"
												labelFor="observacoes"
											/>
											<m:TextArea
												id="observacoes"
												growing="true"
												value="{oModelCreateOrder>/comments}"
												showExceededText="true"
												maxLength="254"
												liveChange="handleLiveChange"
												valueLiveUpdate="true"
												width="40rem"
												rows="4"
												enabled="{oModelCreateOrder>/Enabled}"
												editable="{oModelCreateOrder>/Editable}"
											/>
										</m:FlexBox>
									</layout:VerticalLayout>
								</layout:HorizontalLayout>
							</m:VBox>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection
				titleUppercase="false"
				title="{i18n>logistics}"
			>
				<subSections>
					<ObjectPageSubSection
						title="{i18n>logistics}"
						titleUppercase="false"
						mode="Expanded"
					>
						<blocks>
							<layout:HorizontalLayout allowWrapping="true">
								<layout:VerticalLayout class="sapUiMediumMarginEnd">
									<m:Label
										text="{i18n>deliveryPoint}"
										labelFor="pontoEntrega"
									/>
									<m:TextArea
										id="pontoEntrega"
										width="20rem"
										rows="6"
										editable="false"
										value="{oModelCreateOrder>/devileryPoint}"
									/>
									<m:Label
										text="{i18n>sendType}"
										labelFor="tipoEnvio"
									/>
									<m:Input
										id="tipoEnvio"
										width="20rem"
										type="Text"
										showSuggestion="true"
										showValueHelp="true"
										valueHelpRequest="onShippingTypeHelpRequest"
										change="onShippingTypeSetManual"
										enabled="{oModelCreateOrder>/Enabled}"
										editable="{oModelCreateOrder>/Editable}"
										busy="{oModelCreateOrder>/sendTypeBusy}"
										value="{oModelCreateOrder>/ShippingName}"
									/>
									<m:Label
										text="{i18n>representative}"
										labelFor="representante"
									/>
									<m:Input
										id="representante"
										width="20rem"
										type="Text"
										showSuggestion="true"
										showValueHelp="true"
										valueHelpRequest="onAgentTypeHelpRequest"
										change="onAgentHelpSetManual"
										enabled="{oModelCreateOrder>/Enabled}"
										editable="{oModelCreateOrder>/Editable}"
										value="{oModelCreateOrder>/AgentName}"
									/>
								</layout:VerticalLayout>
							</layout:HorizontalLayout>
							<layout:HorizontalLayout allowWrapping="true">
								<layout:VerticalLayout class="sapUiMediumMarginEnd">
									<m:Label
										text="{i18n>payTo}"
										labelFor="pagarA"
									/>
									<!--campo BilltoDefault do BusinessPartners-->
									<m:Select
										id="pagarA"
										width="20rem"
										forceSelection="false"
										change="onPayToSelect"
										selectedKey="{oModelCreateOrder>/pagarAKey}"
										enabled="{oModelCreateOrder>/PayToEnabled}"
										editable="{oModelCreateOrder>/Editable}"
										items="{path: 'oModelCreateOrder>/PayToList'}"
										busy="{oModelCreateOrder>/PayToBusy}"
									>
										<core:Item
											key="{oModelCreateOrder>BilltoDefault}"
											text="{oModelCreateOrder>BilltoDefault}"
										/>
									</m:Select>
								</layout:VerticalLayout>
								<layout:VerticalLayout class="sapUiMediumMarginEnd">
									<m:TextArea
										id="pagarATextArea"
										width="20rem"
										value="{oModelCreateOrder>/BillToText}"
										busy="{oModelCreateOrder>/BillToBusy}"
										
										rows="8"
										editable="false"
									/>
								</layout:VerticalLayout>
							</layout:HorizontalLayout>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection
				titleUppercase="false"
				title="{i18n>accounting}"
			>
				<subSections>
					<ObjectPageSubSection
						title="{i18n>accounting}"
						titleUppercase="false"
						mode="Expanded"
					>
						<blocks>
							<layout:HorizontalLayout allowWrapping="true">
								<layout:VerticalLayout class="sapUiMediumMarginEnd">
									<m:Label
										text="{i18n>diaryObs}"
										labelFor="obsDiario"
									/>
									<m:TextArea
									width="20rem"
									rows="6"
									editable="false"
									value="{oModelCreateOrder>/Diary}"
								/>
									<m:Label
										text="{i18n>paymentConditions}"
										labelFor="condPagamento"
									/>
									<m:Select
										id="codPgto"
										width="20rem"
										selectedKey="{oModelCreateOrder>/condicaoPgtoKey}"
										forceSelection="false"
										items="{ path: 'oModelCreateOrder>/condPgtoData'}"
										busy="{oModelCreateOrder>/condPgtoBusy}"
										enabled="{oModelCreateOrder>/Enabled}"
										editable="{oModelCreateOrder>/Editable}"
									>
										<core:Item
											key="{oModelCreateOrder>GroupNumber}"
											text="{oModelCreateOrder>PaymentTermsGroupName}"
										/>
									</m:Select>
									<m:Label
										text="{i18n>paymentForm}"
										labelFor="formaPagamento"
									/>
									<m:Select
										id="formaPagamento"
										width="20rem"
										selectedKey="{oModelCreateOrder>/formaPagamentoKey}"
										forceSelection="false"
										items="{ path: 'oModelCreateOrder>/formaPagamento'}"
										busy="{oModelCreateOrder>/formPgtoBusy}"
										enabled="{oModelCreateOrder>/Enabled}"
										editable="{oModelCreateOrder>/Editable}"
									>
										<core:Item
											key="{oModelCreateOrder>PaymentMethodCode}"
											text="{oModelCreateOrder>Description}"
										/>
									</m:Select>
								</layout:VerticalLayout>
							</layout:HorizontalLayout>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection
				titleUppercase="false"
				title="{i18n>approvals}"
				visible="{oModelCreateOrder>/approvalsVisible}"
			>
				<subSections>
					<ObjectPageSubSection
						title="{i18n>approvals}"
						titleUppercase="false"
						mode="Expanded"
					>
						<blocks>
							<m:FlexBox
								direction="Column"
								alignItems="Start"
							>
								<m:Label
									text="{i18n>approvalStatus}"
									labelFor="statusApp"
								/>
								<m:Input
									width="20rem"
									value="{oModelCreateOrder>/status}"
									editable="false"
								/>
								<m:List
									width="20rem"
									class="sapUiSmallMarginTop"
									items="{oModelCreateOrder>/Approvals}"
									includeItemInSelection="true"
									busy="{oModelCreateOrder>/ApprovalsBusy}"
								>
									<!--<StandardListItem title="{Approvals>Status}" description="{Approvals>Name}" icon="{Approvals>Icon}" info="{Approvals>Date}"/>-->
									<m:CustomListItem>
										<m:HBox
											alignItems="Center"
											justifyContent="Start"
										>
											<core:Icon
												size="1.7rem"
												src="{path: 'oModelCreateOrder>U_Resu_work_indi', formatter: '.formatter.iconApprovalStatusFormatter'}"
												color="#0854a0"
												class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom"
											/>
											<m:VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom">
												<m:Label
													class="label1"
													text="{path: 'oModelCreateOrder>U_Resu_work_indi', formatter: '.formatter.statusApprovalStatusFormatter'}"
													design="Bold"
												/>
												<m:Label
													class="label2"
													text="Justificativa: {oModelCreateOrder>U_Observacao}"
												/>
												<m:Label
													class="sapUiSmallMarginTop label3"
													text="{path: 'oModelCreateOrder>U_NomeAprovador', formatter: '.formatter.nameApprovalStatusFormatter'} - {path: 'oModelCreateOrder>U_Data_resu_indi', formatter: '.formatter.dateApprovalStatusFormatter'}"
												/>
											</m:VBox>
										</m:HBox>
									</m:CustomListItem>
								</m:List>
							</m:FlexBox>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection
				titleUppercase="false"
				title="{i18n>attachments}"
			>
				<subSections>
					<ObjectPageSubSection
						title="Anexos"
						titleUppercase="false"
						mode="Expanded"
					>
						<blocks>
							<m:UploadCollection
								id="UploadCollection"
								items="{ path: 'oModelCreateOrder>/Anexos', templateShareable: false }"
								busy="{oModelCreateOrder>/AnexosBusy}"
								maximumFilenameLength="100"
								maximumFileSize="2"
								multiple="true"
								change="onChangeUploadCollection"
								fileDeleted="onFileDeleted"
								uploadComplete="onFileUploadComplete"
								filenameLengthExceed="onFilenameLengthExceed"
								selectionChange="onDownloadItem"
								fileSizeExceed="onFileSizeExceed"
								typeMissmatch="onFileTypeMissmatch"
								beforeUploadStarts="onBeforeUploadStarts"
								mode="SingleSelectMaster"
								uploadEnabled="{oModelCreateOrder>/Editable}"
							>
								<m:toolbar>
									<m:OverflowToolbar id="myId">
										<m:Title id="attachmentTitle"/>
										<m:ToolbarSpacer/>
										<m:UploadCollectionToolbarPlaceholder/>
									</m:OverflowToolbar>
								</m:toolbar>
								<m:items>
									<m:UploadCollectionItem
										id="fileItemId"
										documentId="{oModelCreateOrder>fileName}"
										fileName="{oModelCreateOrder>fileName}"
										mimeType="{oModelCreateOrder>Filemimetype}"
										enableEdit="false"
										visibleEdit="false"
										enableDelete="{oModelCreateOrder>/Editable}"
										visibleDelete="true"
										selected="{oModelCreateOrder>fileSelected}"
									/>
								</m:items>
							</m:UploadCollection>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection
				titleUppercase="false"
				title="{i18n>udf}"
			>
				<subSections>
					<ObjectPageSubSection
						title="{i18n>udf}"
						titleUppercase="false"
						mode="Expanded"
					>
						<blocks id="udfSection">
							<m:VBox id="idPnl"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
</mvc:View>
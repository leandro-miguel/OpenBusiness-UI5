<core:FragmentDefinition fragmentName="contractNumHelpDialog"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:layout="sap.ui.layout"
	xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" >
	<Dialog title="{i18n>agreementRowNumber}" class="sapUiContentPadding" contentHeight="80%" >
		<content>
			<OverflowToolbar>
				<SearchField search=".onContractNumHelpSearch" width="100%" />
			</OverflowToolbar>
			<List
				id="contractNumList"
				items="{ContractNumList>/value}"
				mode="SingleSelectLeft"
                busy="{oModel>/ContractBusy}"
				selectionChange="onContractHelpSave"
				includeItemInSelection="true">
				<StandardListItem
					title="{ContractNumList>BPCode}"
					description="{ContractNumList>BPName}"
					selected="{ContractNumList>U_Nome_CategSelected}" />
			</List>
		</content>
		<buttons>
			<Button id="ContractNumPrevious" icon="sap-icon://arrow-left" press="onContractNumHelpPrevious" type="Emphasized" enabled="false"/>
			<Button id="ContractNumNext" icon="sap-icon://arrow-right" press="onContractNumHelpNext" type="Emphasized"/>
			<Button id="ContractNumCancel" text="{i18n>cancel}" press="onContractNumHelpClose" type="Ghost"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>
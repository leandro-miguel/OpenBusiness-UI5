<core:FragmentDefinition fragmentName="reqNumHelpDialog"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:layout="sap.ui.layout"
	xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" >
	<Dialog title="{i18n>requisitionNum}" class="sapUiContentPadding" contentHeight="80%">
		<content>
			<OverflowToolbar>
				<SearchField search=".onReqNumHelpSearch" width="100%" />
			</OverflowToolbar>
			<List
				id="reqNumList"
				items="{oModel>/RequisitionsList}"
				busy="{oModel>/RequisitionsListBusy}"
				mode="MultiSelect"
				includeItemInSelection="true">
				<StandardListItem
					title="{oModel>DocEntryDocumento}"
					selected="{oModel>RequestSelected}"/>
			</List>
		</content>
		<buttons>
			<Button icon="sap-icon://arrow-left" press="onReqNumHelpPrevious" type="Emphasized" enabled="{oModel>/previousDialogButtonEnabled}"/>
			<Button icon="sap-icon://arrow-right" press="onReqNumHelpNext" type="Emphasized" enabled="{oModel>/nextDialogButtonEnabled}"/>
			<Button text="{i18n>save}" press="onReqNumHelpSave" type="Emphasized"/>
			<Button text="{i18n>cancel}" press="onReqNumHelpClose" type="Ghost"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>
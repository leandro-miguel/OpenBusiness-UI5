<core:FragmentDefinition fragmentName="partnerMultiHelpDialog"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:layout="sap.ui.layout"
	xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" >
	<Dialog title="{i18n>vendor}" class="sapUiContentPadding" contentHeight="80%" >
		<content>
			<OverflowToolbar>
				<SearchField search=".onPartnerMultiHelpSearch" width="100%" />
			</OverflowToolbar>
			<List
				id="partnerMultiList"
				items="{oModel>/vendorList}"
				mode="MultiSelect"
                busy="{oModel>/vendorListBusy}"
				includeItemInSelection="true">
				<StandardListItem
					title="{oModel>CardCode}"
					description="{oModel>CardName}" />
			</List>
		</content>
		<buttons>
			<Button id="PartnerMultiPrevious" icon="sap-icon://arrow-left" press="onPartnerMultiHelpPrevious" type="Emphasized" enabled="false"/>
			<Button id="PartnerMultiNext" icon="sap-icon://arrow-right" press="onPartnerMultiHelpNext" type="Emphasized"/>
			<Button id="PartnerMultiConfirm" text="{i18n>save}" press="onPartnerMultiHelpSave" type="Emphasized"/>
			<Button id="PartnerMultiCancel" text="{i18n>cancel}" press="onPartnerMultiHelpClose" type="Ghost"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>